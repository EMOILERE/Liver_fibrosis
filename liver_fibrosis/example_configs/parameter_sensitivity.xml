<?xml version="1.0" encoding="UTF-8"?>
<!-- 参数敏感性分析配置 - 用于系统性研究参数影响 -->
<simulation_parameters>
    <basic_settings>
        <simulation_time>1440</simulation_time> <!-- 24小时适中时间 -->
        <time_steps>
            <diffusion_dt>0.01</diffusion_dt>
            <mechanics_dt>0.1</mechanics_dt>
            <phenotype_dt>6</phenotype_dt>
        </time_steps>
        <domain>
            <size_x>700</size_x>
            <size_y>700</size_y>
            <resolution>20</resolution>
        </domain>
        <output>
            <save_interval>60</save_interval>
            <svg_interval>120</svg_interval> <!-- 减少SVG保存频率 -->
        </output>
    </basic_settings>

    <cell_population>
        <initial_cell_count>80</initial_cell_count> <!-- 适中的细胞数 -->
        <initial_activation_fraction>0.0</initial_activation_fraction>
        <cell_biology>
            <quiescent_cycle_time>2000</quiescent_cycle_time>
            <activated_cycle_time>1000</activated_cycle_time>
            <apoptosis_rate>0.0001</apoptosis_rate>
            <motility>
                <quiescent_speed>0.2</quiescent_speed>
                <activated_speed>1.0</activated_speed>
            </motility>
        </cell_biology>
    </cell_population>

    <microenvironment>
        <TGF_beta1>
            <concentration>10.0</concentration> <!-- 基准浓度，可变参数 -->
            <diffusion_coefficient>50000.0</diffusion_coefficient>
            <decay_rate>0.01</decay_rate>
        </TGF_beta1>
        <exosomes>
            <concentration>50.0</concentration> <!-- 基准浓度，可变参数 -->
            <diffusion_coefficient>5000.0</diffusion_coefficient>
            <decay_rate>0.005</decay_rate>
        </exosomes>
        <fibrosis_markers>
            <alpha_SMA>
                <diffusion_coefficient>10000.0</diffusion_coefficient>
                <decay_rate>0.02</decay_rate>
            </alpha_SMA>
            <collagen_I>
                <diffusion_coefficient>5000.0</diffusion_coefficient>
                <decay_rate>0.005</decay_rate>
            </collagen_I>
        </fibrosis_markers>
    </microenvironment>

    <cell_activation>
        <TGF_beta1_response>
            <activation_threshold>5.0</activation_threshold> <!-- 关键敏感参数 -->
            <activation_rate>0.01</activation_rate> <!-- 关键敏感参数 -->
            <deactivation_rate>0.005</deactivation_rate>
            <hill_coefficient>2.0</hill_coefficient>
        </TGF_beta1_response>
        <activated_phenotype>
            <alpha_SMA_production>
                <quiescent_rate>0.1</quiescent_rate>
                <activated_rate>2.0</activated_rate>
            </alpha_SMA_production>
            <collagen_I_production>
                <quiescent_rate>0.05</quiescent_rate>
                <activated_rate>1.5</activated_rate>
            </collagen_I_production>
        </activated_phenotype>
    </cell_activation>

    <exosome_miRNA_system>
        <exosome_uptake>
            <uptake_rate>1.0</uptake_rate> <!-- 敏感参数 -->
            <uptake_threshold>0.1</uptake_threshold>
            <miRNA_release_efficiency>0.1</miRNA_release_efficiency> <!-- 敏感参数 -->
        </exosome_uptake>
        <miRNA_kinetics>
            <degradation_rate>0.01</degradation_rate> <!-- 敏感参数 -->
            <effect_threshold>0.1</effect_threshold> <!-- 敏感参数 -->
            <max_effect>0.8</max_effect>
            <hill_coefficient>2.0</hill_coefficient>
        </miRNA_kinetics>
        <miRNA_specificity>
            <miR_455_3p>
                <alpha_SMA_inhibition>0.9</alpha_SMA_inhibition>
                <collagen_I_inhibition>0.2</collagen_I_inhibition>
            </miR_455_3p>
            <miR_148a_5p>
                <alpha_SMA_inhibition>0.2</alpha_SMA_inhibition>
                <collagen_I_inhibition>0.9</collagen_I_inhibition>
            </miR_148a_5p>
        </miRNA_specificity>
    </exosome_miRNA_system>

    <synergy_mechanism>
        <synergy_threshold>0.1</synergy_threshold> <!-- 最敏感的参数 -->
        <max_synergy_factor>2.5</max_synergy_factor> <!-- 最敏感的参数 -->
        <synergy_model>geometric</synergy_model>
        <synergy_delay>30</synergy_delay>
        <synergy_saturation>1.0</synergy_saturation>
    </synergy_mechanism>

    <experimental_conditions>
        <current_experiment>dual_miRNA_1_1</current_experiment>
        <experiment_groups>
            <control>
                <TGF_beta1_enabled>false</TGF_beta1_enabled>
                <exosomes_enabled>false</exosomes_enabled>
            </control>
            <positive_model>
                <TGF_beta1_enabled>true</TGF_beta1_enabled>
                <exosomes_enabled>false</exosomes_enabled>
            </positive_model>
            <miR_455_3p>
                <TGF_beta1_enabled>true</TGF_beta1_enabled>
                <exosomes_enabled>true</exosomes_enabled>
                <miR_455_3p_ratio>1.0</miR_455_3p_ratio>
                <miR_148a_5p_ratio>0.0</miR_148a_5p_ratio>
            </miR_455_3p>
            <miR_148a_5p>
                <TGF_beta1_enabled>true</TGF_beta1_enabled>
                <exosomes_enabled>true</exosomes_enabled>
                <miR_455_3p_ratio>0.0</miR_455_3p_ratio>
                <miR_148a_5p_ratio>1.0</miR_148a_5p_ratio>
            </miR_148a_5p>
            <dual_miRNA_1_1>
                <TGF_beta1_enabled>true</TGF_beta1_enabled>
                <exosomes_enabled>true</exosomes_enabled>
                <miR_455_3p_ratio>0.5</miR_455_3p_ratio>
                <miR_148a_5p_ratio>0.5</miR_148a_5p_ratio>
            </dual_miRNA_1_1>
        </experiment_groups>
    </experimental_conditions>

    <advanced_parameters>
        <random_seed>42</random_seed> <!-- 固定种子以便重现 -->
        <numerical_precision>
            <tolerance>1e-6</tolerance>
            <max_iterations>1000</max_iterations>
        </numerical_precision>
        <performance>
            <omp_threads>6</omp_threads>
            <memory_limit>8</memory_limit>
        </performance>
        <debugging>
            <verbose_output>true</verbose_output> <!-- 详细输出便于分析 -->
            <log_level>2</log_level>
        </debugging>
    </advanced_parameters>

    <!-- 参数敏感性分析注释 -->
    <!-- 
    关键敏感参数列表（按重要性排序）:
    1. synergy_threshold (0.05-0.2) - 最影响协同效应
    2. max_synergy_factor (1.5-4.0) - 协同强度
    3. activation_threshold (3.0-8.0) - 细胞活化灵敏度
    4. miRNA_release_efficiency (0.05-0.2) - miRNA递送效率
    5. TGF_beta1_concentration (5.0-20.0) - 纤维化驱动强度
    6. miRNA_degradation_rate (0.005-0.02) - miRNA持续时间
    7. effect_threshold (0.05-0.2) - miRNA作用灵敏度
    8. activation_rate (0.005-0.02) - 活化动力学
    
    建议的参数扫描范围:
    - synergy_threshold: [0.05, 0.08, 0.1, 0.12, 0.15, 0.2]
    - max_synergy_factor: [1.5, 2.0, 2.5, 3.0, 3.5, 4.0]
    - activation_threshold: [3.0, 5.0, 7.0, 10.0]
    - TGF_beta1_concentration: [5.0, 10.0, 15.0, 20.0]
    -->
</simulation_parameters>
